ition());\r\n    return $(id).position();\r\n}\r\nexport { animateText, addSpansToText, addHtmlToClass}",["64","65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":3,"column":9,"nodeType":"72","messageId":"73","endLine":3,"endColumn":14},{"ruleId":"70","severity":1,"message":"74","line":1,"column":28,"nodeType":"72","messageId":"73","endLine":1,"endColumn":37},{"ruleId":"70","severity":1,"message":"75","line":3,"column":10,"nodeType":"72","messageId":"73","endLine":3,"endColumn":21},{"ruleId":"70","severity":1,"message":"76","line":3,"column":10,"nodeType":"72","messageId":"73","endLine":3,"endColumn":19},{"ruleId":"77","severity":1,"message":"78","line":13,"column":25,"nodeType":"79","endLine":13,"endColumn":37},{"ruleId":"70","severity":1,"message":"80","line":2,"column":5,"nodeType":"72","messageId":"73","endLine":2,"endColumn":12},{"ruleId":"81","severity":1,"message":"82","line":16,"column":23,"nodeType":"83","messageId":"84","endLine":16,"endColumn":25},{"ruleId":"81","severity":1,"message":"85","line":36,"column":23,"nodeType":"83","messageId":"84","endLine":36,"endColumn":25},{"ruleId":"70","severity":1,"message":"86","line":57,"column":10,"nodeType":"72","messageId":"73","endLine":57,"endColumn":25},{"ruleId":"66","replacedBy":"87"},{"ruleId":"68","replacedBy":"88"},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"no-unused-vars","'FaCog' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.","'animateText' is defined but never used.","'GoArchive' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-va